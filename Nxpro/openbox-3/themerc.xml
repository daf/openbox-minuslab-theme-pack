<?xml version="1.0"?>
<openbox_theme version="1" xmlns="http://openbox.org/themerc">
  <dimensions>
    <handle>2</handle>
    <padding x="1" y="1"/>
  </dimensions>
  <window>
    <border>
      <width>1</width>
      <primary r="140" g="140" b="140" a="255"/>
    </border>
    <clientpadding x="0" y="0"/>
    <justify>left</justify>
    <active>
      <clientpadding r="210" g="210" b="210" a="255"/>
      <label>
        <text>
          <primary r="0" g="0" b="0" a="255"/>
        </text>
        <style>parentrelative</style>
      </label>
      <buttons>
        <unpressed>
          <image r="102" g="102" b="102" a="255"/>
          <style>parentrelative</style>
        </unpressed>
        <pressed>
          <image r="0" g="0" b="0" a="255"/>
          <style>parentrelative</style>
        </pressed>
        <disabled>
          <image r="204" g="204" b="204" a="255"/>
          <style>parentrelative</style>
        </disabled>
        <toggled-unpressed>
          <image r="68" g="68" b="68" a="255"/>
          <style>parentrelative</style>
        </toggled-unpressed>
      </buttons>
      <titlebar>
        <style>raised gradient vertical bevel1</style>
        <primary r="221" g="221" b="221" a="255"/>
        <secondary r="202" g="202" b="202" a="255"/>
        <border r="102" g="102" b="102" a="255"/>
      </titlebar>
      <handle>
        <style>flat solid</style>
        <primary r="210" g="210" b="210" a="255"/>
      </handle>
      <grip>
        <style>flat solid</style>
        <primary r="210" g="210" b="210" a="255"/>
      </grip>
    </active>
    <inactive>
      <clientpadding r="64" g="72" b="80" a="255"/>
      <label>
        <text>
          <primary r="136" g="136" b="136" a="255"/>
        </text>
        <style>parentrelative</style>
      </label>
      <buttons>
        <unpressed>
          <image r="179" g="179" b="179" a="255"/>
          <style>parentrelative</style>
        </unpressed>
        <pressed>
          <image r="102" g="102" b="102" a="255"/>
          <style>parentrelative</style>
        </pressed>
        <disabled>
          <image r="204" g="204" b="204" a="255"/>
          <style>parentrelative</style>
        </disabled>
        <toggled-unpressed>
          <image r="85" g="85" b="85" a="255"/>
          <style>parentrelative</style>
        </toggled-unpressed>
      </buttons>
      <titlebar>
        <style>flat</style>
        <primary r="215" g="215" b="215" a="255"/>
      </titlebar>
      <handle>
        <style>flat</style>
        <primary r="215" g="215" b="215" a="255"/>
      </handle>
      <grip>
        <style>flat</style>
        <primary r="215" g="215" b="215" a="255"/>
      </grip>
    </inactive>
  </window>
  <menu>
    <border>
      <width>1</width>
      <primary r="140" g="140" b="140" a="255"/>
    </border>
    <justify>left</justify>
    <title>
      <text>
        <primary r="0" g="0" b="0" a="255"/>
      </text>
      <style>raised gradient vertical bevel1</style>
      <primary r="221" g="221" b="221" a="255"/>
      <secondary r="202" g="202" b="202" a="255"/>
    </title>
    <inactive>
      <primary r="0" g="0" b="0" a="255"/>
    </inactive>
    <disabled>
      <primary r="136" g="136" b="136" a="255"/>
    </disabled>
    <active>
      <text>
        <primary r="102" g="102" b="102" a="255"/>
      </text>
      <style>raised gradient vertical bevel3</style>
      <primary r="232" g="232" b="232" a="255"/>
      <secondary r="234" g="234" b="234" a="255"/>
    </active>
    <entries>
      <style>flat solid</style>
      <primary r="215" g="215" b="215" a="255"/>
    </entries>
  </menu>
</openbox_theme>
